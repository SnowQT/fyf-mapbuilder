{
  "compilerOptions": {
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "outDir": "../build/transpiled/",
    "typeRoots": [ "node_modules/@types" ]
  },
  "include": [
    "@types/**/*.d.ts",
  ],

  "exclude": [
    "./node_modules/**/*"
  ],

  "files": [
    "src/MapBuilderApp.tsx"
  ]
}